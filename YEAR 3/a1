#include <iostream>
using namespace std;

int main () {
    reverseNum(1);
}

int reverseNum(int num){
    int rev = 0, rem = 0;
    while(num > 0){
        rem = num%10;
        rev = rev*10 + rem;
        num/=10;
    }
    return rev;
}

int reverseNum2(int num){
    String value = to_string(num), rev = "";
    for(int i = 0; i < value.length(); i++){
        String curr = value.charAt(i) + "";
        rev = curr + rev;
    }
    return rev;
}

int GCD(int num1, int num2){
    int highestDiv = 0;
    if(num1 < num2){
        highestDiv = num1
    }else if(num1>num2){
        highestDiv = num2
    }else{
        return num1;
    }
    
    while(highestDiv > 0){
        if(num1%highestDiv == 0 && num2%highestDiv == 0){
            break;
        }
        highestDiv--;
    }
    return highestDiv;
}

int palNum(int num){
    int rev = reverseNum(num);
    bool check = false;
    if(rev == num){
        check = true;
    }
    return check;
}